<%- include("partials/header") -%>
    <h1>Home</h1>
    <p><%= homeStartingContent %></p>
    <% posts.forEach((post)=>{ %>
       <h1><%= post.title; %></h1>
       <p>
           <%= post.content.substr(0,100) + " ..."; %>
           <a href="/posts/<%= post.title %>">
               Read More
           </a>
       </p>
    <% }); %>
    <a href="/compose"><button type="button" class="btn btn-primary">New Post</button></a>
<%- include("partials/footer") -%>